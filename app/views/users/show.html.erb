<h1>Welcome <%= current_user.username %>!</h1>

<h3>Your Jams!</h3>
<h5><%= link_to "Add Jam", new_song_path(user_id: current_user.id) %></h5>

<table>
    <% current_user.songs.each do |song| %>
  <tr>
    <td>Song Title</td>
    <td>Artist</td>
  </tr>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.artist %></td>
  </tr>
    <% end %>

</table>
